#graph_bound{ :style => "height:400px; width:100%; position:relative; " }
  #graph
  %div{ :style => "position:absolute; top:20px; right:20px;" }
    %input.time{ :time => "1", :type => "button", :value => "D" }
    %input.time{ :time => "7", :type => "button", :value => "W" }
    %input.time{ :time => "30", :type => "button", :value => "M" }
    %input.time{ :time => "365", :type => "button", :value => "Y" }
    |
    %input#zoomin{ :type => "button", :value => "+" }
    %input#zoomout{ :type => "button", :value => "-" }
    %input#left{ :type => "button", :value => "<" }
    %input#right{ :type => "button", :value => ">" }
  #loading{ :style => "position:absolute; top:0px; left:0px; width:100%; height:100%; background-color: rgba(255,255,255,0.5);" }
  %h3{ :style => "position:absolute; top:00px; left:40px;" }
    %span#stat

